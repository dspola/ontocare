@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix agePe: <http://www.stiima.cnr.it/AgeIT-PNRR/Person#> .
@base <http://www.stiima.cnr.it/AgeIT-PNRR/Person> .

<http://www.stiima.cnr.it/AgeIT-PNRR/Person> rdf:type owl:Ontology ;
                                              rdfs:comment "\"Person\" module of the Age-IT ontology" ,
                                                           "Age-IT PNRR Research project - SPOKE 9" ,
                                                           """Contact: Daniele Spoladore (STIIMA Cnr)
daniele.spoladore@stiima.cnr.it""" ;
                                              rdfs:seeAlso "SPOKE9 Website: https://ageit.eu/wp/s-p-o-k-e-9/" .

#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  http://www.stiima.cnr.it/AgeIT-PNRR/Person#isHealthConditionOf
agePe:isHealthConditionOf rdf:type owl:ObjectProperty ;
                          owl:inverseOf agePe:isInHealthCondition .


###  http://www.stiima.cnr.it/AgeIT-PNRR/Person#isInHealthCondition
agePe:isInHealthCondition rdf:type owl:ObjectProperty ;
                          rdfs:domain agePe:Person ;
                          rdfs:range agePe:HealthCondition .


#################################################################
#    Data properties
#################################################################

###  http://www.stiima.cnr.it/AgeIT-PNRR/Person#age
agePe:age rdf:type owl:DatatypeProperty ,
                   owl:FunctionalProperty ;
          rdfs:domain agePe:Person ;
          rdfs:range xsd:int .


###  http://www.stiima.cnr.it/AgeIT-PNRR/Person#assessedOnDate
agePe:assessedOnDate rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;
                     rdfs:domain agePe:HealthCondition ;
                     rdfs:range xsd:date .


###  http://www.stiima.cnr.it/AgeIT-PNRR/Person#dateOfBirth
agePe:dateOfBirth rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;
                  rdfs:domain agePe:Person ;
                  rdfs:range xsd:date .


###  http://www.stiima.cnr.it/AgeIT-PNRR/Person#name
agePe:name rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;
           rdfs:domain agePe:Person ;
           rdfs:range xsd:string .


###  http://www.stiima.cnr.it/AgeIT-PNRR/Person#surname
agePe:surname rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain agePe:Person ;
              rdfs:range xsd:string .


#################################################################
#    Classes
#################################################################

###  http://www.stiima.cnr.it/AgeIT-PNRR/Person#HealthCondition
agePe:HealthCondition rdf:type owl:Class ;
                      owl:equivalentClass [ rdf:type owl:Restriction ;
                                            owl:onProperty agePe:assessedOnDate ;
                                            owl:someValuesFrom xsd:dateTime
                                          ] .


###  http://www.stiima.cnr.it/AgeIT-PNRR/Person#Person
agePe:Person rdf:type owl:Class ;
             owl:equivalentClass [ rdf:type owl:Restriction ;
                                   owl:onProperty agePe:isInHealthCondition ;
                                   owl:someValuesFrom agePe:HealthCondition
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty agePe:age ;
                                   owl:someValuesFrom xsd:int
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty agePe:name ;
                                   owl:someValuesFrom xsd:string
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty agePe:surname ;
                                   owl:someValuesFrom xsd:string
                                 ] .


